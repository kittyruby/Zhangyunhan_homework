package main

import "fmt"

func add(x, y int) int {
	return x + y
}

func main() {
	num1 := 5
	num2 := 7
	sum := add(num1, num2)
	fmt.Println("Sum:", sum)
}



//lv2
package main

import (
	"fmt"
	"math"
)

func calculateArea(radius float64) float64 {
	return math.Pi * radius * radius
}

func main() {
	radius := 5.0
	area := calculateArea(radius)
	fmt.Println("Area:", area)
}
